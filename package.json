{
	"name": "azotui",
	"version": "0.0.1",
	"license": "MIT",
	"bin": {
		"azotui": "dist/src/cli.js"
	},
	"type": "module",
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"start": "node dist/src/cli.js",
		"test": "prettier --check . && xo && ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"ffmpeg-static": "^5.2.0",
		"ffprobe-static": "^3.1.0",
		"fluent-ffmpeg": "^2.1.3",
		"ink": "^4.1.0",
		"ink-big-text": "^2.0.0",
		"ink-gradient": "^3.0.0",
		"ink-spawn": "^0.1.4",
		"ink-spinner": "^5.0.0",
		"meow": "^11.0.0",
		"react": "^18.2.0",
		"sharp": "^0.34.2"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"xo": "^0.53.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config",
	"engines": {
		"node": ">=20"
	},
	"volta": {
		"node": "20.19.3"
	}
}
